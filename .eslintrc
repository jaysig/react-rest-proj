{
    "extends": ["plugin:meteor/recommended","airbnb"],
    "env": {
        "browser": true,
        "node": true,
        "es6": true,
        "jquery": true,
        "mongo": true,
        "meteor": true
    },

    "plugins": [
        "react",
        "meteor"
    ],
    "rules":{
      "prefer-const": 0,
      "one-var": 0,
      "func-names": 0,
      "no-else-return": 0,
      "no-underscore-dangle": 0,
      }
}
